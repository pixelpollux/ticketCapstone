    <!---- Ticket Display --->
    <div id="ticketDisplay" class="container-fluid">
        <!-- <div class=""> -->
            <button id="createTicketButton">Create Ticket</button>
        <!-- </div> -->
      
      <!-- <button id="edit">Edit Ticket</button> -->
      <div class="row">
        <div class="col-11 container-fluid justify-content-center">
          <!----- Heading ----->
          <table id="ticket-table">
            <tr>
              <th>ID#</th>
              <th>TITLE</th>
              <th>CREATED BY</th>
              <th>DATE CREATED</th>
              <th>DATE DUE</th>
              <th>ASSIGNED TO</th>
              <th></th>
            </tr>

            <!-- ticket loop -->
            <% for(var i = 0; i < tickets.length; i++) {%>
                <tr>
                  <!-- Output details from the iterated quote object -->
                    <td><%= i+1 %></td>
                    <td><%= tickets[i].ticketTitle %></td>
                    <td></td>
                    <td></td>
                    <td></td>
                    <td></td>
                    <td>
                        <btn class="btn btn-danger">Edit</btn>
                        <btn class="btn btn-danger">Delete</btn>
                    </td>
                </tr>
            <% } %>
            <!-- end ticket loop -->
            <tr>
              <td class="td">10001</td>
              <td>Tom</td>
              <td>M</td>
              <td>30</td>
              <td>M</td>
              <td>30</td>
              <td>
                <btn class="btn btn-danger">Edit</btn>
                <btn class="btn btn-danger">Delete</btn>
              </td>
            </tr>
          </table>
        </div>
      </div>
