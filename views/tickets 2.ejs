  <div id="mainWrapper" class="wrap col-12">
    <!----Charts & Graphs --->
    <div id="charts" class="container-fluid justify-content-center col-11">
      <div class="row">
        <div id="graph1" class="col-sm-4">
          <div class="card">
            <div class="card-body">
              <h5 class="card-title">Chart 1</h5>
              <p class="card-text">
                With supporting text below as a natural lead-in to additional
                content.
              </p>
            </div>
          </div>
        </div>
        <div id="graph2" class="col-sm-4">
          <div class="card">
            <div class="card-body">
