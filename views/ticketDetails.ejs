        <div class="container-fluid justify-content-center col-11">
            <div class="row col-12">
                <div class="col-sm-12">
                    <div class="card">
                        <div class="card-body">
                            <div class="id-tab">#000000</div>
                            <h5 class="card-title">Chart 1</h5>
                            <p class="card-text">With supporting text below as a natural lead-in to additional content.
                            </p>
                            <% for(var i = 0; i < tickets.length; i++) {%>
                                <!-- Output details from the iterated quote object -->
                                <table>
                                <tr>
                                    <td>
                                          <btn class="btn btn-danger">Edit</btn>
                                          <btn class="btn btn-danger">Delete</btn>
                                    </td>
                                </tr>
                                <tr>
                                    <td>id: </td>
                                    <td><%= tickets[i]._id %></td>
                                </tr>
                                <tr>
                                    <td>title: </td>
                                    <td><%= tickets[i].ticketTitle %></td>
